#include <iostream>
using namespace std;

float suiza (int s1)
{
	switch  (s1)
	{
		int p,fp,tt,td;
		case 1:
			cout <<"Introduzca el número de huéspedes"<<endl;
			cin>>p;
			cout <<"Introduzca días de permanencia"<<endl;
			cin>>fp;
			tt=p*1500;
		td=fp*80;
		tt=tt+td;
		return tt;
		break;
		
		//	int p,fp,tt,td;
		case 2:
			cout <<"Introduzca el número de huéspedes"<<endl;
			cin>>p;
			cout <<"Introduzca días de permanencia"<<endl;
			cin>>fp;
			tt=p*1000;
		td=fp*70;
		tt=tt+td;
		return tt;
		break;
		case 3:
			//int p,fp,tt,td;
				cout <<"Introduzca el número de huéspedes"<<endl;
			cin>>p;
			cout <<"Introduzca días de permanencia"<<endl;
			cin>>fp;
			tt=p*1200;
		td=fp*75;
		tt=tt+td;
		return tt;
		break;
	}
}
